{
  "expo": {
    "name": "BelsoftOnboarding",
    "slug": "BelsoftOnboarding",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.png",
    "scheme": "belsoftonboarding",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#0000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sladelaroy.BelsoftOnboarding"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        
    "expo-splash-screen",
    {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "transparent" 
    }
      ]
      
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "de00c819-a682-4e9a-9d29-720c60d3dad8"
      }
    }
  }
}
